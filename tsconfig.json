{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true
  },
  "exclude": [
    "proxy/**"
  ],
  "vueCompilerOptions": {
    "plugins": [
      "@vue-macros/volar/define-models",
      "@vue-macros/volar/short-vmodel"
    ],
    "vueMacros": {
      "shortVmodel": {
        "prefix": "$"
      },
    }
  }
}